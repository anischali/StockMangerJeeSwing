package com.gest.beans;

public class ArticleOrder extends Order{
	private static long id = 0;

	
	
	public ArticleOrder() {
		super();
		id++;
	}
	/**
	 * @return the id
	 */
	public static long getId() {
		return id;
	}

	/**
	 * @param id the id to set
	 */
	public static void setId(long id) {
		ArticleOrder.id = id;
	}
	
	
}
